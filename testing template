import time
import random

numbers = (random.randint(1, 5), random.randint(1, 5), random.randint(1, 5))
answer = numbers[0]+numbers[1]*numbers[2]

print("Calculate, you have 5 seconds!")
print("-> ",numbers[0],"+",numbers[1],"*",numbers[2])
start = time.time()
choice = input("-> ")
end = time.time()
if end - start > 5:
    print("Too slow, out of time!")
elif answer != int(choice):
    print("Incorrect")
else:
    print("Correct")

